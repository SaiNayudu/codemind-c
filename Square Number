#include<stdio.h>
int main()
{
    int n,s=0,cnt=0;
    scanf("%d",&n);
    for(int i=0;i<n;i++)
    {
        for(int j=0;j<n;j++)
        {
                s=i*i+j*j;
                if(n==s)
                {
                    cnt++;
                    break;
                }
        }
    }
    if(cnt==0)
    {
        printf("False");
    }
    else
    {
        printf("True");
    }
}
